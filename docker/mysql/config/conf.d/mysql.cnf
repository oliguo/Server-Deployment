#dev.oli local vol to docker-mysql
[mysql]
# Set utf8mb4 by default: https://mathiasbynens.be/notes/mysql-utf8mb4
default-character-set = utf8mb4
#https://github.com/jackfrued/Python-100-Days/blob/master/Day91-100/93.MySQL%E6%80%A7%E8%83%BD%E4%BC%98%E5%8C%96.md
slow_query_log=ON
slow_query_log_file=/var/lib/mysql/slow_query.log
long_query_time=1
